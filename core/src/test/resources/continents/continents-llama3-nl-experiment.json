{
  "name": "Continents llama3 NL Experiment",
  "metrics": [
    "CellPrecision",
    "CellSimilarityPrecision",
    "CellRecall",
    "CellSimilarityRecall",
    "CellF1Score",
    "CellSimilarityF1Score",
    "TupleCardinality",
    "TupleOrder",
    "TupleConstraint",
    "TupleSimilarityConstraint"
  ],
  "dbms": "postgres",
  "operatorsConfig": {
    "scan": {
      "queryExecutor": "ollama-llama3-nl",
      "maxIterations": 1,
      "naturalLanguagePrompt": "List the name, gender and birth year of female actors born after 1980."
    }
  },
  "query": {
    "sql": "select name from target.continents c order by c.name",
    "schema": {
      "name": "continents",
      "schema": "target",
      "username": "pguser",
      "password": "pguser",
      "tables": [
        {
          "tableName": "continents",
          "attributes": [
            {
              "name": "name",
              "type": "STRING",
              "nullable": false,
              "key": true
            },
            {
              "name": "area",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "population",
              "type": "STRING",
              "nullable": false
            }
          ]
        }
      ]
    },
    "results": "/continents/continent"
  }
}