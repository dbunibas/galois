{
  "name": "Continents-{{QN}} llama3 Table Experiment",
  "metrics": [
    "CellPrecision",
    "CellSimilarityPrecision",
    "CellRecall",
    "CellSimilarityRecall",
    "CellF1Score",
    "CellSimilarityF1Score",
    "TupleCardinality",
    "TupleConstraint",
    "TupleSimilarityConstraint"
  ],
  "dbms": "postgres",
  "operatorsConfig": {
    "scan": {
      "queryExecutor": "ollama-llama3-table",
      "maxIterations": 1
    }
  },
  "query": {
    "sql": "{{QUERY-SQL}}",
    "schema": {
      "name": "continents",
      "schema": "target",
      "username": "pguser",
      "password": "pguser",
      "tables": [
        {
          "tableName": "continents",
          "attributes": [
            {
              "name": "name",
              "type": "STRING",
              "nullable": false,
              "key": true
            },
            {
              "name": "area_km2",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "population",
              "type": "STRING",
              "nullable": false
            }
          ]
        }
      ]
    },
    "results": "/continents/continent"
  }
}