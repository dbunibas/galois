  "query": {
    "sql": "{{QUERY_SQL}}",
    "schema": {
      "name": "academic_journal",
      "schema": "target",
      "username": "pguser",
      "password": "pguser",
      "tables": [
        {
          "tableName": "academic_journal",
          "attributes": [
            {
              "name": "homepage",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "jid",
              "type": "INTEGER",
              "nullable": false,
              "key": true
            },
            {
              "name": "name",
              "type": "STRING",
              "nullable": false
            }
          ]
        }    
      ]
    },
    "results": "/llm-bench/spider1/academic"
  }
}