{
  "name": "Geo llama3-table Experiment",
  "metrics": [
    "CellPrecision",
    "CellSimilarityPrecision",
    "CellRecall",
    "CellSimilarityRecall",
    "CellF1Score",
    "CellSimilarityF1Score",
    "TupleCardinality",
    "TupleConstraint",
    "TupleSimilarityConstraint"
  ],
  "dbms": "postgres",
  "operatorsConfig": {
    "scan": {
      "queryExecutor": "togetherai-llama3-key-scan",
      "maxIterations": 1
    }
  },
  "query": {
    "sql": "SELECT state_name, capital, population FROM target.usa_state where population>500000",
    "schema": {
      "name": "usa_geo",
      "schema": "target",
      "username": "pguser",
      "password": "pguser",
      "tables": [
        {
          "tableName": "usa_state",
          "attributes": [
            {
              "name": "state_name",
              "type": "STRING",
              "nullable": false,
              "key": true
            },
            {
              "name": "population",
              "type": "INTEGER",
              "nullable": true
            },
            {
              "name": "area",
              "type": "FLOAT",
              "nullable": true
            },
            {
              "name": "country_name",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "capital",
              "type": "STRING",
              "nullable": true
            },
            {
              "name": "density",
              "type": "FLOAT",
              "nullable": true
            }
          ]
        }
      ]
    },
    "results": "/geo_data/geo"
  }
}