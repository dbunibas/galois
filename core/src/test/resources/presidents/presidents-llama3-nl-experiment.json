{
  "name": "Presidents-{{QN}} llama3-nl Experiment",
  "metrics": [
    "CellPrecision",
    "CellSimilarityPrecision",
    "CellRecall",
    "CellSimilarityRecall",
    "CellF1Score",
    "CellSimilarityF1Score",
    "TupleCardinality",
    "TupleConstraint",
    "TupleSimilarityConstraint"
  ],
  "dbms": "postgres",
  "operatorsConfig": {
    "scan": {
      "queryExecutor": "togetherai-llama3-nl",
      "maxIterations": 10,
      "naturalLanguagePrompt": "{{PROMPT}}"
    }
  },
  "query": {
    "sql": "{{QUERY_SQL}}",
    "schema": {
      "name": "all_president",
      "schema": "target",
      "username": "pguser",
      "password": "pguser",
      "tables": [
        {
          "tableName": "international_presidents",
          "attributes": [
            {
              "name": "name",
              "type": "STRING",
              "nullable": false,
              "key": true
            },
            {
              "name": "start_year",
              "type": "INTEGER",
              "nullable": false
            },
            {
              "name": "end_year",
              "type": "INTEGER",
              "nullable": false
            },
            {
              "name": "cardinal_number",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "party",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "country",
              "type": "STRING",
              "nullable": false
            }
          ]
        }
      ]
    },
    "results": "/presidents/presidents_data"
  }
}