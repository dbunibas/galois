{
  "name": "Actors llama3-table Experiment",
  "metrics": [
    "CellPrecision",
    "CellSimilarityPrecision",
    "CellRecall",
    "CellSimilarityRecall",
    "CellF1Score",
    "CellSimilarityF1Score",
    "TupleCardinality",
    "TupleOrder",
    "TupleConstraint",
    "TupleSimilarityConstraint"
  ],
  "dbms": "postgres",
  "operatorsConfig": {
    "scan": {
      "queryExecutor": "ollama-llama3-table",
      "maxIterations": 1
    }
  },
  "query": {
    "sql": "select * from target.actor a where a.gender = 'Male'",
    "schema": {
      "name": "dummy_actors",
      "schema": "target",
      "username": "pguser",
      "password": "pguser",
      "tables": [
        {
          "tableName": "actor",
          "attributes": [
            {
              "name": "name",
              "type": "STRING",
              "nullable": false,
              "key": true
            },
            {
              "name": "gender",
              "type": "STRING",
              "nullable": false
            },
            {
              "name": "birth_year",
              "type": "INTEGER",
              "nullable": false
            }
          ]
        }
      ]
    },
    "results": "/dummy-filtered-actors/actor"
  }
}